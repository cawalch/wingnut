{
  "name": "wingnut",
  "version": "0.3.6",
  "description": "API validation middleware builder based on Open API V3 specs",
  "main": "./dist/index.js",
  "scripts": {
    "prepublish": "pnpm build",
    "test": "vitest",
    "preinstall": "npx only-allow pnpm",
    "coverage": "vitest run --coverage",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node14 --outdir=dist --packages=external && tsc --project tsconfig.build.json",
    "lint": "eslint . --ext .ts"
  },
  "author": "cawalch@pm.me",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-istanbul": "^1.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "esbuild": "^0.20.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "ajv": "^8.12.0",
    "express": "^4.18.2"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cawalch/wingnut.git"
  },
  "bugs": {
    "url": "https://github.com/cawalch/wingnut/issues"
  },
  "keywords": [
    "api",
    "ajv",
    "validation",
    "middleware",
    "openapi",
    "swagger",
    "typescript",
    "express.js"
  ],
  "packageManager": "pnpm@8.7.1",
  "homepage": "https://github.com/cawalch/wingnut#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
